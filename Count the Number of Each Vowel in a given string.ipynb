   "source": [
    "# Count the Number of Each Vowel in a given string"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "337cce35-9fef-4c8f-bdbc-b1190346b24d",
   "metadata": {},
   "outputs": [
    {
     "name": "stdin",
     "output_type": "stream",
     "text": [
      "Enter your word aeioujjkkkkk\n"
     ]
    },
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "No. of vowels are 5\n"
     ]
    }
   ],
   "source": [
    "name = input(\"Enter your word\").lower()\n",
    "v = 0\n",
    "i = 0\n",
    "while i < len(name):\n",
    "    if name[i] in \"aeiou\":\n",
    "        v += 1\n",
    "    i += 1\n",
    "else:\n",
    "    print(f\"No. of vowels are {v}\")"
   ]
  },
